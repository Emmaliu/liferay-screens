<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:liferay="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">

	<TextView
		style="@style/label"
		android:text="@string/webview_label" />

	<com.liferay.mobile.screens.webcontent.display.WebContentDisplayScreenlet
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		liferay:articleId="@string/article_id_structured"
		liferay:layoutId="@layout/webcontentdisplaystructured_example"
		liferay:offlinePolicy="REMOTE_FIRST"
		liferay:structureId="@string/structure_id_structured" />

</LinearLayout>